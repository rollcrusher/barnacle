  "scripts": {
    "ng": "node_modules/.bin/ng",
    "back:start": "node server.js",
    "front:start": "node_modules/.bin/ng serve",
    "build": "tsc",
    "test:e2e": "node_modules/.bin/nightwatch -c ./nightwatch.conf.js --env chrome",
    "test:unit": "rm -rf reports & node_modules/.bin/ng test",
    "test:coverage": "rm -rf reports & node_modules/.bin/ng test --code-coverage",
    "test:api:allure": "node_modules/.bin/mocha --reporter mocha-allure-reporter test/api/spec",
    "test:api:list": "node_modules/.bin/mocha --reporter list back/tests/api/spec",
    "test:report": "node_modules/allure-commandline/bin/allure generate allure-results --clean -o reports/allure-report",
    "reports:clean": "rm -rf reports",
    "code:tslint": "node_modules/.bin/tslint --project ./tslint.json ./front/**/*.ts",
    "code:eslint": "eslint back/**/*.js --quiet",
    "code:analyse": "plato -r -d reports/test-analyse -e .eslintrc.json -t \"barnacle\" -x .json back/**/*.js test/**/*.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "db:start": "mongod --dbpath /usr/local/var/mongodb"
  }
}
